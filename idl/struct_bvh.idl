  [Value] BBox3fa bounds();
};
 AABBNode4 implements BaseNode4;

[NoDelete]
interface AABBNode4MB {
  [Value] BBox3fa bounds(unsigned long i);
  [Value] BBox3fa bounds();
};
AABBNode4MB implements BaseNode4;

[NoDelete]
interface OBBNode4 {
};
OBBNode4 implements BaseNode4;

[NoDelete]
interface OBBNodeMB4 {
};
OBBNodeMB4 implements BaseNode4;
[NoDelete]
interface QAABBNode4 {
  [Value] BBox3fa bounds(unsigned long i);
